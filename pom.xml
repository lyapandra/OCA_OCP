    <dependencies>
        <dependency>
            <groupId>org.webjars.bower</groupId>
            <artifactId>cldr-data</artifactId>
            <version>28.0.1</version>
        </dependency>
        <dependency>
            <groupId>com.ibm.icu</groupId>
